"*":
  "advanced-open-file":
    fuzzyMatch: true
  "autocomplete-python":
    pythonPaths: "/usr/local/bin/python3"
    useKite: false
    useSnippets: "required"
  core:
    disabledPackages: [
      "exception-reporting"
      "metrics"
      "fuzzy-finder"
    ]
    fileEncoding: "utf8"
    openEmptyEditorOnStart: true
    packagesWithKeymapsDisabled: [
      "spell-check"
      "markdown-preview"
      "fuzzy-finder"
      "autocomplete-python"
      "highlight-selected"
      "advanced-open-file"
      "todo-show"
    ]
    telemetryConsent: "no"
    themes: [
      "one-dark-ui"
      "tomorrow-night-eighties-syntax"
    ]
  editor:
    fontSize: 12
    scrollPastEnd: true
    showIndentGuide: true
    showInvisibles: false
    softWrap: true
    softWrapHangingIndent: 4
    tabLength: 4
    tabType: "soft"
    themes: [
      "one-dark-ui"
      "zenburn"
    ]
  "exception-reporting":
    userId: "63aff982-a3bd-b4e8-17ff-fb49af5b9aa7"
  "fold-functions":
    autofoldGrammars: [
      "Python"
    ]
    skipAutofoldWhenOnlyOneFunction: true
  "fuzzy-finder":
    preserveLastSearch: true
    searchAllPanes: true
  "git-plus":
    general: {}
  "highlight-selected": {}
  linter:
    errorPanelHeight: 69
  "linter-pep8":
    ignoreErrorCodes: [
      "E501"
    ]
  "linter-python-pep8":
    ignoreErrorCodes: "E501"
    pep8DirToExecutable: "/usr/local/bin/"
  "markdown-preview":
    useGitHubStyle: true
  "one-dark-ui":
    layoutMode: "Compact"
    tabSizing: "Minimum"
  "one-light-ui":
    fontSize: 10
    layoutMode: "Compact"
    tabSizing: "Minimum"
  "package-sync":
    createOnChange: true
    forceOverwrite: true
  "spell-check":
    addKnownWords: true
    locales: [
      "de-DE"
      "en-US"
    ]
    useLocales: false
  tabs:
    enableVcsColoring: true
  "tabs-to-spaces":
    onSave: "untabify"
  "todo-show":
    openListInDirection: "down"
    saveOutputAs: "Table"
    showInTable: [
      "Text"
      "Path"
    ]
    sortBy: "Type"
    statusBarIndicator: true
  "tree-view": {}
  welcome:
    showOnStartup: false
  whitespace:
    removeTrailingWhitespace: true
